<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mini.entity">
	<class name="ImportBookInfo" table="book_import_infor">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="isbn10" column="isbn10" />
		<property name="name" column="name" />		
		<property name="position" column="position" />
		<property name="importdate" column="importdate" />
		<property name="source" column="source" />
		<property name="number" column="number" />		
		<property name="price" column="price" />
		<property name="onlysign" column="onlysign" />
		<property name="state" column="state" />
		<property name="classifi" column="classifi" />
		
	</class>
</hibernate-mapping>
