<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mini.entity">

	<class name="UserImage" table="user_image">

		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="usernumber" column="user_number" />
		<property name="imagename" column="image_name" />		
		<property name="feedbackname" column="feedback_name" />
		<property name="feedbackcontent" column="feedback_content" />
		<property name="feedbacktime" column="feedback_time" />

	</class>
</hibernate-mapping>
