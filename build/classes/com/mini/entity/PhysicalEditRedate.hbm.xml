<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mini.entity">
	<class name="PhysicalEditRedate" table="physical_editRedate">
		<id name="id" column="id">
			<generator class="native" />
		</id>

		<property name="usernumber" column="user_number" />
		<property name="username" column="user_name" />
		<property name="editRedate" column="edit_Redate" />
		<property name="importdate" column="import_date" />
		<property name="physicalplan" column="physical_plan" />
		<property name="flag" column="flag" />
		<property name="physicalposition" column="physical_position" />

	</class>
</hibernate-mapping>
