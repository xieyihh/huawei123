<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mini.entity">
	<class name="Activity" table="table_activity">
		<id name="id" column="activity_id">
			<generator class="native" />
		</id>
		<property name="name" column="activity_name" />
		<property name="description" column="activity_description" />
		<property name="initiatorId" column="activity_initiatorId" />
		<property name="initiator" column="activity_initiator" />
		<property name="date" column="activity_date" />
		<property name="applyDeadline" column="activity_applyDeadline" />
		<property name="startDate" column="activity_startDate" />
		<property name="endDate" column="activity_endDate" />
		<property name="property" column="activity_property" />
		<property name="attendees" column="activity_attendees" />
		<property name="attendeesProperty" column="activity_attendeesProperty" />
		<property name="max" column="activity_max" />
		<property name="size" column="activity_size" />
	</class>
</hibernate-mapping>
