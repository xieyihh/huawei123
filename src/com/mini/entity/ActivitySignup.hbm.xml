<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mini.entity">
	<class name="ActivitySignup" table="activity_signup">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="activityid" column="activity_id" />
		<property name="userid" column="user_id" />
		<property name="flag" column="signup_flag" />
		<property name="content" column="signup_content" />
		<property name="remark" column="remark" />		

	</class>

</hibernate-mapping>
